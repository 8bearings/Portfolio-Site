{
  "name": "vanilla-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Simple workflow starter for building vanilla JS apps",
  "main": "main.js",
  "scripts": {
    "dev": "npm run clean && parcel index.html --out-dir development -p 2121",
    "build": "rimraf ./dist && parcel build index.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf ./.cache",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "keywords": [
    "parcel"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
